    "scripts": {
        "start": "npm run build && node ./dist-web/server --cport 8081 --sport 8080",
        "start:pm2": "npm run build && npm run clean:sourcemap && pm2 restart pm2.json --only fleet-yuubari",
        "start:dev": "npm run pm2:dev",
        "deploy": "git pull && node ./deploy",


        "prebuild": "npm run clean:dist && node ./scripts/database.js",
        "build": "npm run dist:client && npm run dist:server && npm run copy:public",
        "build:app": "",


        "clean:dist": "rimraf ./dist-web",
        "clean:sourcemap": "node ./scripts/clean_sourcemap",
        "copy:public": "node ./scripts/copyfiles",


        "dev:client": "node ./scripts/database.js && node ./webpack/enter --stage client --env dev --cport 3002",
        "dev:server": "node ./webpack/enter --stage server --env dev --cport 3002",
        "dev:run": "node ./dist-web/server --cport 3002 --sport 3000",


        "dist:server": "node ./webpack/enter --stage server --env dist",
        "dist:client": "node ./webpack/enter --stage client --env dist",
        "dist:run": "npm run dist:server && npm run dev:run",
        "dist": "npm run build",


        "pm2:dev:client": "node ./scripts/database.js && pm2 restart pm2.json --only fleet-dev-client",
        "pm2:dev:server": "pm2 restart pm2.json --only fleet-dev-server",
        "pm2:dev:run": "pm2 restart pm2.json --only fleet-dev-run --watch",
        "pm2:dev": "npm run pm2:dev:client && npm run pm2:dev:server && npm run pm2:dev:run",
        "pm2:clean": "pm2 delete fleet-dev-client fleet-dev-server fleet-dev-run",
        "pm2:clean:log": ""
    },




    "window": {
        "frame": true,
        "toolbar": false,
        "width": 1280,
        "height": 720,
        "icon": "./app/appicon.png",
        "position": "center",
        "min_width": 640,
        "min_height": 480,
        "as_desktop": false,
        "resizable": true,
        "always-on-top": false,
        "fullscreen": false,
        "show_in_taskbar": true,
        "show": false,
        "kiosk": false,
        "transparent": false
    },
    "platformOverrides": {
        "win": {
            "debug": false,
            "window": {
                "frame": false
            }
        },
        "win32": {
            "debug": false,
            "window": {
                "frame": false
            }
        },
        "osx": {
            "debug": false,
            "window": {
                "frame": true
            }
        },
        "linux": {
            "debug": false,
            "window": {
                "frame": true
            }
        }
    },




